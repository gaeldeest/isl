module Make : functor () -> sig
  type id
  type value
  type space
  type local_space
  type aff
  type pw_aff
  type multi_aff
  type pw_multi_aff
  type matrix
  type constrnt_type = Equality | Inequality
  type constrnt
  type basic_set
  type set
  type union_set
  type basic_map
  type map
  type union_map

  type dim_type =  [ `Param | `In | `Out | `Set ]
  type noparam = [ `In | `Out | `Set ]

  module Id : Isl_Sigs.ID with
    type t := id
end
